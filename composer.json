{
    "name": "yegortokmakov/phprtest",
    "description": "Simple resources usage tests for PHP applications",
    "keywords": ["test", "performance", "resources"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yegor Tokmakov",
            "email": "yegor.tokmakov@home24.de",
            "homepage": "http://bigdatanotes.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/console": "~2.5",
        "symfony/finder": "~2.2",
        "mindplay/annotations": "1.2.*@dev",
        "phpunit/phpunit-mock-objects": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Phprtest\\": [
                "src/Phprtest"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phprtest\\": [
                "tests/phpunit/Phprtest"
            ]
        }
    },
    "bin": ["bin/phprtest"]
}